{
    "manifest_version": 2,
    "name": "简单时钟",
    "version": "1.0",
    "description": "一个简单的时钟演示",
    "icons": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "我的时钟",
        "default_popup": "time.html"
    }
}